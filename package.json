{
  "name": "times1000",
  "version": "0.1.0",
  "description": "Semi-autonomous AI agent framework",
  "main": "dist/index.js",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "npm run build:server && node server.js",
    "build": "vite build && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --kill-others-on-fail"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.3",
    "mysql2": "^3.13.0",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.3.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5.0.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  }
}
